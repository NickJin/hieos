<Config>

    <HomeCommunity name="home">
        <Property name="HomeCommunityId">urn:oid:1.3.6.1.4.1.21367.2009.1.2.2020</Property>
        <Property name="InitiatingGateway">localig</Property>
        <Property name="RespondingGateway">localrg</Property>
        <Property name="ValidatePatientId">true</Property>

        <!-- Place to find the "codes.xml" file.

             Alternatively, you can set the HIEOSxCodesFile environment variable to include
             the path to the "codes.xml" file such as c:\dev\vhgate\xds-xca\src\xdstoolkit\codes\codes.xml

             -->
        <Property name="CodesLocation">http://localhost:8080/xdsref/codes/codes.xml</Property>

        <!-- This is used to configure the local repository. In the future, we may need to find
        a better way to allow the same instance to support more than one repository ID.  This is really
        only used for the "Provide and Register" transaction.  The Gateway's lookup the proper repository
        by using the "repositoryId" in the associated "Retrieve" requests. -->
        <Property name="LocalRepository">localrepository</Property>

        <!-- ATNA configuration parameters. -->
        <Property name="ATNAPerformAudit">true</Property>
        <!--<Property name="ATNAsyslogHost">localhost</Property>
        <Property name="ATNAsyslogPort">4010</Property>-->

        <!-- NIST ATNA log -->
        <!-- NIST syslog (browser URL: http://129.6.24.109:9080/SyslogBrowser/) -->
        <Property name="ATNAsyslogHost">129.6.24.109</Property>
        <Property name="ATNAsyslogPort">8087</Property>
        <!-- AXOLOTL ATNA log -->
        <!--<Property name="ATNAsyslogHost">10.243.0.52</Property>
        <Property name="ATNAsyslogPort">514</Property>-->

        <!-- SOAP configuration parameters. -->
        <Property name="SOAPTimeOutInMilliseconds">45000</Property> <!-- 45 seconds -->

        <!-- Used to configure underlying ebXML implementation. -->
        <Property name="ebXMLHost">localhost</Property>
        <Property name="ebXMLPort">8080</Property>
        <Property name="ebXMLUsernamePassword">regop:regop</Property>
        <Property name="ebXMLRestService">/ebxmlrr/registry/rest</Property>

        <!-- Used to configure ADT database -->
        <!-- ADTURL of the form: jdbc:postgresql://host:port/database -->
        <Property name="ADTURL">jdbc:postgresql://localhost:5432/adt</Property>
        <Property name="ADTUsername">adt</Property>
        <Property name="ADTPassword">xdsadt</Property>

        <!-- Registry database configuration (to support Patient Identity Feed). -->
        <Property name="ebXMLdbURL">jdbc:postgresql://localhost:5432/registry</Property>
        <Property name="ebXMLdbUsername">ebxmlrr</Property>
        <Property name="ebXMLdbPassword">ebxmlrr</Property>

        <!-- Log file parameters -->
        <Property name="LogURL">jdbc:postgresql://localhost/log2</Property>
        <Property name="LogUsername">logs</Property>
        <Property name="LogPassword">xdslogs</Property>

        <!-- LogMaxBytes determines how many bytes to store in the logger for "Other" messages.
             Set this value to -1 if you do not want any restrictions on the number of bytes to
             store in the log.
        -->
        <Property name="LogMaxBytes">5000</Property>

        <!-- Other global properties will go here. -->
    </HomeCommunity>

