
<config>
    <!-- JNDI resource name for EMPI database -->
    <jndi-resource-name>jdbc/hieos-empi</jndi-resource-name>
    
    <!-- JBOSS 
    <jndi-resource-name>java:/jdbc/hieos-empi</jndi-resource-name>-->

    <!-- Name of candidate finder to use -->
    <candidate-finder>com.vangent.hieos.empi.match.BlockCandidateFinder</candidate-finder>

    <!-- Name of match algorithm to use. -->
    <match-algorithm>com.vangent.hieos.empi.match.FRILMatchAlgorithm</match-algorithm>

    <!-- when enabled, will validate codes in HL7V3 messages against codes.xml configuration. -->
    <validate-codes-enabled>false</validate-codes-enabled>

    <!-- Enterprise ID configuration items. -->
    <euid-config>
        <euid-assign-enabled>false</euid-assign-enabled>
        <euid-universalid>1.3.6.1.4.1.21367.13.20.9999</euid-universalid>
        <euid-universalid-type>ISO</euid-universalid-type>
    </euid-config>

    <!-- Available transform functions -->
    <transform-functions>
        <transform-function>
            <name>DoubleMetaphone</name>
            <class>com.vangent.hieos.empi.transform.DoubleMetaphoneTransformFunction</class>
            <parameters>
                <parameter>
                    <name></name>
                    <value></value>
                </parameter>
                <parameter>
                    <name></name>
                    <value></value>
                </parameter>
            </parameters>
        </transform-function>
        <transform-function>
            <name>Metaphone</name>
            <class>com.vangent.hieos.empi.transform.MetaphoneTransformFunction</class>
            <parameters>
                <parameter>
                    <name></name>
                    <value></value>
                </parameter>
                <parameter>
                    <name></name>
                    <value></value>
                </parameter>
            </parameters>
        </transform-function>
        <transform-function>
            <name>RefinedSoundex</name>
            <class>com.vangent.hieos.empi.transform.RefinedSoundexTransformFunction</class>
            <parameters>
                <parameter>
                    <name></name>
                    <value></value>
                </parameter>
                <parameter>
                    <name></name>
                    <value></value>
                </parameter>
            </parameters>
        </transform-function>
        <transform-function>
            <name>Soundex</name>
            <class>com.vangent.hieos.empi.transform.SoundexTransformFunction</class>
            <parameters>
                <parameter>
                    <name></name>
                    <value></value>
                </parameter>
                <parameter>
                    <name></name>
                    <value></value>
                </parameter>
            </parameters>
        </transform-function>
        <transform-function>
            <name>Caverphone1</name>
            <class>com.vangent.hieos.empi.transform.Caverphone1TransformFunction</class>
            <parameters>
                <parameter>
                    <name></name>
                    <value></value>
                </parameter>
                <parameter>
                    <name></name>
                    <value></value>
                </parameter>
            </parameters>
        </transform-function>
        <transform-function>
            <name>Caverphone2</name>
            <class>com.vangent.hieos.empi.transform.Caverphone2TransformFunction</class>
            <parameters>
                <parameter>
                    <name></name>
                    <value></value>
                </parameter>
                <parameter>
                    <name></name>
                    <value></value>
                </parameter>
            </parameters>
        </transform-function>
        <transform-function>
            <name>HL7Date</name>
            <class>com.vangent.hieos.empi.transform.HL7DateTransformFunction</class>
        </transform-function>
        <transform-function>
            <name>Prefix</name>
            <class>com.vangent.hieos.empi.transform.PrefixTransformFunction</class>
            <parameters>
                <parameter>
                    <name>prefix-length</name>
                    <value>3</value>
                </parameter>
            </parameters>
        </transform-function>
        <transform-function>
            <name>NormalizeCase</name>
            <class>com.vangent.hieos.empi.transform.NormalizeCaseTransformFunction</class>
        </transform-function>
        <transform-function>
            <name>fuzzyNameSearch</name>
            <class>com.vangent.hieos.empi.transform.FuzzyNameSearchTransformFunction</class>
        </transform-function>
        <transform-function>
            <name>IdentifierTransform</name>
            <class>com.vangent.hieos.empi.transform.IdentifierTransformFunction</class>
            <parameters>
                <parameter>
                    <name></name>
                    <value></value>
                </parameter>
                <parameter>
                    <name></name>
                    <value></value>
                </parameter>
            </parameters>
        </transform-function>
    </transform-functions>

    <!-- Available distance functions -->
    <distance-functions>
        <distance-function>
            <name>Levenshtein</name>
            <class>com.vangent.hieos.empi.distance.LevenshteinDistanceFunction</class>
            <parameters>
                <parameter>
                    <name></name>
                    <value></value>
                </parameter>
                <parameter>
                    <name></name>
                    <value></value>
                </parameter>
            </parameters>
        </distance-function>
        <distance-function>
            <name>ExactMatch</name>
            <class>com.vangent.hieos.empi.distance.ExactMatchDistanceFunction</class>
            <!--<parameters>
                <parameter>
                    <name>truncate-length</name>
                    <value>5</value>
                </parameter>
            </parameters>-->
        </distance-function>
        <distance-function>
            <name>FuzzyMatch</name>
            <class>com.vangent.hieos.empi.distance.FuzzyMatchDistanceFunction</class>
            <!--<parameters>
                <parameter>
                    <name>truncate-length</name>
                    <value>5</value>
                </parameter>
            </parameters>-->
        </distance-function>
    </distance-functions>

    <!-- List of fields to support blocking and matching functions -->
    <fields>
        <field>
            <name>familyNameDoubleMetaphone</name>
            <source-object-path>subjectNames[0].familyName</source-object-path>
            <transform-functions>
                <transform-function>
                    <name>DoubleMetaphone</name>
                </transform-function>
            </transform-functions>
            <match-db-column>family_name_double_metaphone</match-db-column>
        </field>
        <field>
            <name>givenNameDoubleMetaphone</name>
            <source-object-path>subjectNames[0].givenName</source-object-path>
            <transform-functions>
                <transform-function>
                    <name>DoubleMetaphone</name>
                </transform-function>
            </transform-functions>
            <match-db-column>given_name_double_metaphone</match-db-column>
        </field>
        <field>
            <name>familyName</name>
            <source-object-path>subjectNames[0].familyName</source-object-path>
            <transform-functions>
                <transform-function>
                    <name>NormalizeCase</name>
                </transform-function>
            </transform-functions>
            <match-db-column>family_name</match-db-column>
        </field>
        <field>
            <name>givenName</name>
            <source-object-path>subjectNames[0].givenName</source-object-path>
            <transform-functions>
                <transform-function>
                    <name>NormalizeCase</name>
                </transform-function>
            </transform-functions>
            <match-db-column>given_name</match-db-column>
        </field>
        <field>
            <name>birthTime</name>
            <source-object-path>birthTime</source-object-path>
            <transform-functions>
                <transform-function>
                    <name>HL7Date</name>
                </transform-function>
            </transform-functions>
            <match-db-column>birth_time</match-db-column>
        </field>
        <field>
            <name>gender</name>
            <source-object-path>gender.code</source-object-path>
            <transform-functions>
                <transform-function>
                    <name>NormalizeCase</name>
                </transform-function>
            </transform-functions>
            <match-db-column>gender</match-db-column>
        </field>
        <field>
            <name>familyNameSoundex</name>
            <source-object-path>subjectNames[0].familyName</source-object-path>
            <transform-functions>
                <transform-function>
                    <name>Soundex</name>
                </transform-function>
            </transform-functions>
            <match-db-column>family_name_soundex</match-db-column>
        </field>
        <field>
            <name>givenNameSoundex</name>
            <source-object-path>subjectNames[0].givenName</source-object-path>
            <transform-functions>
                <transform-function>
                    <name>Soundex</name>
                </transform-function>
            </transform-functions>
            <match-db-column>given_name_soundex</match-db-column>
        </field>
        <field>
            <name>streetAddressLine1</name>
            <source-object-path>addresses[0].streetAddressLine1</source-object-path>
            <transform-functions>
                <transform-function>
                    <name>NormalizeCase</name>
                </transform-function>
            </transform-functions>
            <match-db-column>street_address_line1</match-db-column>
        </field>
        <field>
            <name>city</name>
            <source-object-path>addresses[0].city</source-object-path>
            <transform-functions>
                <transform-function>
                    <name>NormalizeCase</name>
                </transform-function>
            </transform-functions>
            <match-db-column>city</match-db-column>
        </field>
        <field>
            <name>state</name>
            <source-object-path>addresses[0].state</source-object-path>
            <transform-functions>
                <transform-function>
                    <name>NormalizeCase</name>
                </transform-function>
            </transform-functions>
            <match-db-column>state</match-db-column>
        </field>
        <field>
            <name>postalCode</name>
            <source-object-path>addresses[0].postalCode</source-object-path>
            <transform-functions>
                <transform-function>
                    <name>NormalizeCase</name>
                </transform-function>
            </transform-functions>
            <match-db-column>postal_code</match-db-column>
        </field>
        <field>
            <name>familyNameCaverphone2</name>
            <source-object-path>subjectNames[0].familyName</source-object-path>
            <transform-functions>
                <transform-function>
                    <name>Caverphone2</name>
                </transform-function>
            </transform-functions>
            <match-db-column>family_name_caverphone2</match-db-column>
        </field>
        <field>
            <name>familyNameCaverphone1</name>
            <source-object-path>subjectNames[0].familyName</source-object-path>
            <transform-functions>
                <transform-function>
                    <name>Caverphone1</name>
                </transform-function>
            </transform-functions>
            <match-db-column>family_name_caverphone1</match-db-column>
        </field>
        <field>
            <name>familyNamePrefix</name>
            <source-object-path>subjectNames[0].familyName</source-object-path>
            <transform-functions>
                <transform-function>
                    <name>Prefix</name>
                    <parameters>
                        <parameter>
                            <name>prefix-length</name>
                            <value>2</value>
                        </parameter>
                    </parameters>
                </transform-function>
            </transform-functions>
            <match-db-column>family_name_prefix</match-db-column>
        </field>
        <field>
            <name>familyNameFuzzy</name>
            <supersedes-field>familyName</supersedes-field>
             <!-- default for store-field is "true" -->
            <store-field>false</store-field>
            <source-object-path>subjectNames[0]</source-object-path>
            <transform-functions>
                <transform-function>
                    <name>fuzzyNameSearch</name>
                    <parameters>
                        <parameter>
                            <name>field-name</name>
                            <value>familyName</value>
                        </parameter>
                    </parameters>
                </transform-function>
            </transform-functions>
            <match-db-column>family_name</match-db-column>
        </field>
        <field>
            <name>givenNameFuzzy</name>
            <supersedes-field>givenName</supersedes-field>
            <store-field>false</store-field>
            <source-object-path>subjectNames[0]</source-object-path>
            <transform-functions>
                <transform-function>
                    <name>fuzzyNameSearch</name>
                    <parameters>
                        <parameter>
                            <name>field-name</name>
                            <value>givenName</value>
                        </parameter>
                    </parameters>
                </transform-function>
            </transform-functions>
            <match-db-column>given_name</match-db-column>
        </field>
        <field>
            <name>ssn</name>
            <source-object-path>subject</source-object-path>
            <transform-functions>
                <transform-function>
                    <name>IdentifierTransform</name>
                    <parameters>
                        <parameter>
                            <name>universal-id</name>
                            <value>2.16.840.1.113883.4.1</value>
                        </parameter>
                    </parameters>
                </transform-function>
            </transform-functions>
            <match-db-column>ssn</match-db-column>
        </field>
    </fields>
    
    <match-config-feed>
        <!-- blocking configuration (multi pass) -->
        <blocking-config>
            <blocking-pass>
                <blocking-fields>
                    <blocking-field>
                        <name>ssn</name>
                        <required>true</required>
                    </blocking-field>
                </blocking-fields>
            </blocking-pass>
            <blocking-pass>
                <blocking-fields>
                    <blocking-field>
                        <name>familyNameSoundex</name>
                        <required>true</required>
                    </blocking-field>
                    <blocking-field>
                        <name>givenNameSoundex</name>
                        <required>false</required>
                    </blocking-field>
                    <blocking-field>
                        <name>gender</name>
                        <required>false</required>
                    </blocking-field>
                </blocking-fields>
            </blocking-pass>
            <blocking-pass>
                <blocking-fields>
                    <blocking-field>
                        <name>streetAddressLine1</name>
                        <required>true</required>
                    </blocking-field>
                    <blocking-field>
                        <name>city</name>
                        <required>false</required>
                    </blocking-field>
                    <blocking-field>
                        <name>state</name>
                        <required>false</required>
                    </blocking-field>
                    <blocking-field>
                        <name>postalCode</name>
                        <required>false</required>
                    </blocking-field>
                </blocking-fields>
            </blocking-pass>
        </blocking-config>
        <accept-threshold>0.9</accept-threshold>
        <reject-threshold>1.0</reject-threshold>
        <match-fields>
            <match-field>
                <name>givenName</name>
                <accept-threshold>0.8</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>3.0</weight>
                <distance-function>
                    <name>Levenshtein</name>
                </distance-function>
            </match-field>
            <match-field>
                <name>familyName</name>
                <accept-threshold>0.8</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>5.0</weight>
                <distance-function>
                    <name>Levenshtein</name>
                </distance-function>
            </match-field>
            <match-field>
                <name>birthTime</name>
                <accept-threshold>1.0</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>3.0</weight>
                <distance-function>
                    <name>ExactMatch</name>
                </distance-function>
            </match-field>
            <match-field>
                <name>gender</name>
                <accept-threshold>1.0</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>4.0</weight>
                <distance-function>
                    <name>ExactMatch</name>
                </distance-function>
            </match-field>
            <!-- this implies that SSN is required - should set flag here (if not required; i.e. no penalty). -->
            <!-- FIXME!!!
            <match-field>
                <name>ssn</name>
                <accept-threshold>1.0</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>4.0</weight>
                <distance-function>
                    <name>ExactMatch</name>
                </distance-function>
            </match-field>-->
            <match-field>
                <name>streetAddressLine1</name>
                <accept-threshold>0.8</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>1.0</weight>
                <distance-function>
                    <name>Levenshtein</name>
                </distance-function>
            </match-field>
            <match-field>
                <name>city</name>
                <accept-threshold>0.8</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>1.0</weight>
                <distance-function>
                    <name>Levenshtein</name>
                </distance-function>
            </match-field>
            <match-field>
                <name>state</name>
                <accept-threshold>0.8</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>1.0</weight>
                <distance-function>
                    <name>Levenshtein</name>
                </distance-function>
            </match-field>
            <match-field>
                <name>postalCode</name>
                <accept-threshold>0.8</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>1.0</weight>
                <distance-function>
                    <name>Levenshtein</name>
                </distance-function>
            </match-field>
        </match-fields>
    </match-config-feed>

    <match-config-find>
         <!-- blocking configuration (multi pass) -->
        <blocking-config>
            <blocking-pass>
                <blocking-fields>
                    <blocking-field>
                        <name>ssn</name>
                        <required>true</required>
                    </blocking-field>
                </blocking-fields>
            </blocking-pass>
            <blocking-pass>
                <blocking-fields>
                    <blocking-field>
                        <name>familyNameSoundex</name>
                        <required>true</required>
                    </blocking-field>
                    <blocking-field>
                        <name>givenNameSoundex</name>
                        <required>false</required>
                    </blocking-field>
                    <blocking-field>
                        <name>gender</name>
                        <required>false</required>
                    </blocking-field>
                </blocking-fields>
            </blocking-pass>
            <blocking-pass>
                <blocking-fields>
                    <blocking-field>
                        <name>streetAddressLine1</name>
                        <required>true</required>
                    </blocking-field>
                    <blocking-field>
                        <name>city</name>
                        <required>false</required>
                    </blocking-field>
                    <blocking-field>
                        <name>state</name>
                        <required>false</required>
                    </blocking-field>
                    <blocking-field>
                        <name>postalCode</name>
                        <required>false</required>
                    </blocking-field>
                </blocking-fields>
            </blocking-pass>
            <!-- Only used to support Connectathon - not a good notion. -->
            <!-- would need to create an index on this field. -->
            <!--
            <blocking-pass>
                <blocking-fields>
                    <blocking-field>
                        <name>birthTime</name>
                        <required>true</required>
                    </blocking-field>
                </blocking-fields>
            </blocking-pass>
            -->
            <blocking-pass>
                <blocking-fields>
                    <blocking-field>
                        <name>familyNameFuzzy</name>
                        <required>true</required>
                    </blocking-field>
                    <blocking-field>
                        <name>givenNameFuzzy</name>
                        <required>false</required>
                    </blocking-field>
                </blocking-fields>
            </blocking-pass>
        </blocking-config>
        <accept-threshold>0.9</accept-threshold>
        <reject-threshold>1.0</reject-threshold>
        <match-fields>
             <!-- new to support SRCH -->
            <match-field>
                <name>givenNameFuzzy</name>
                <accept-threshold>1.0</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>3.0</weight>
                <distance-function>
                    <name>FuzzyMatch</name>
                </distance-function>
            </match-field>
            <match-field>
                <name>familyNameFuzzy</name>
                <accept-threshold>1.0</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>3.0</weight>
                <distance-function>
                    <name>FuzzyMatch</name>
                </distance-function>
            </match-field>
            <match-field>
                <name>givenName</name>
                <accept-threshold>0.8</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>3.0</weight>
                <distance-function>
                    <name>Levenshtein</name>
                </distance-function>
            </match-field>
            <match-field>
                <name>familyName</name>
                <accept-threshold>0.8</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>5.0</weight>
                <distance-function>
                    <name>Levenshtein</name>
                </distance-function>
            </match-field>
            <match-field>
                <name>birthTime</name>
                <accept-threshold>1.0</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>3.0</weight>
                <distance-function>
                    <name>ExactMatch</name>
                </distance-function>
            </match-field>
            <match-field>
                <name>gender</name>
                <accept-threshold>1.0</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>4.0</weight>
                <distance-function>
                    <name>ExactMatch</name>
                </distance-function>
            </match-field>
            <!-- currently this will not really have an impact since if identifiers are supplied (in search), the
                 FRIL matching algorithm is not employed.  Only identifiers are used in this case for lookup. -->
            <match-field>
                <name>ssn</name>
                <accept-threshold>1.0</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>4.0</weight>
                <distance-function>
                    <name>ExactMatch</name>
                </distance-function>
            </match-field>
            <match-field>
                <name>streetAddressLine1</name>
                <accept-threshold>0.8</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>1.0</weight>
                <distance-function>
                    <name>Levenshtein</name>
                </distance-function>
            </match-field>
            <match-field>
                <name>city</name>
                <accept-threshold>0.8</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>1.0</weight>
                <distance-function>
                    <name>Levenshtein</name>
                </distance-function>
            </match-field>
            <match-field>
                <name>state</name>
                <accept-threshold>0.8</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>1.0</weight>
                <distance-function>
                    <name>Levenshtein</name>
                </distance-function>
            </match-field>
            <match-field>
                <name>postalCode</name>
                <accept-threshold>0.8</accept-threshold>
                <reject-threshold>0.0</reject-threshold>
                <weight>1.0</weight>
                <distance-function>
                    <name>Levenshtein</name>
                </distance-function>
            </match-field>
        </match-fields>
    </match-config-find>

    <update-notification-enabled>false</update-notification-enabled>

    <!-- List of cross reference consumers -->
    <cross-reference-consumers>
        <!--
        <cross-reference-consumer>
            <name>TIANI</name>
            <enabled>true</enabled>
            <device-id>1.3.6.1.4.1.21367.13.30.6</device-id>
            <device-name>1.3.6.1.4.1.21367.13.30.6</device-name>
            <identifier-domains>
                <identifier-domain>
                    <universal-id>1.3.6.1.4.1.21367.13.20.3000</universal-id>
                    <universal-id-type>ISO</universal-id-type>
                    <namespace-id>IHE-BLUE</namespace-id>
                </identifier-domain>
            </identifier-domains>
        </cross-reference-consumer>
        -->
        <cross-reference-consumer>
            <name>NIST PIX PDQ Tool</name>
            <enabled>true</enabled>
            <device-id>2.16.840.1.113883.3.72.6.5.100.207</device-id>
            <device-name>2.16.840.1.113883.3.72.6.1</device-name>
            <!-- List of interested identifier domains -->
            <identifier-domains>
                <identifier-domain>
                    <universal-id>1.3.6.1.4.1.21367.13.20.1000</universal-id>
                    <universal-id-type>ISO</universal-id-type>
                    <namespace-id>IHE_1000</namespace-id>
                </identifier-domain>
                <identifier-domain>
                    <universal-id>2.16.840.1.113883.3.72.5.9.1</universal-id>
                    <universal-id-type>ISO</universal-id-type>
                    <namespace-id>NIST2010</namespace-id>
                </identifier-domain>
                <identifier-domain>
                    <universal-id>2.16.840.1.113883.3.72.5.9.2</universal-id>
                    <universal-id-type>ISO</universal-id-type>
                    <namespace-id>NIST2010-2</namespace-id>
                </identifier-domain>
