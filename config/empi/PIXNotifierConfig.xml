                    <namespace-id>NIST2010-2</namespace-id>
                </identifier-domain>
                <identifier-domain>
                    <universal-id>2.16.840.1.113883.3.72.5.9.3</universal-id>
                    <universal-id-type>ISO</universal-id-type>
                    <namespace-id>NIST2010-3</namespace-id>
