                </identifier-domain>
                <identifier-domain>
                    <universal-id>2.16.840.1.113883.3.72.5.9.2</universal-id>
                    <universal-id-type>ISO</universal-id-type>
                    <namespace-id>NIST2010-2</namespace-id>
                </identifier-domain>
                <identifier-domain>
                    <universal-id>2.16.840.1.113883.3.72.5.9.3</universal-id>
                    <universal-id-type>ISO</universal-id-type>
                    <namespace-id>NIST2010-3</namespace-id>
                </identifier-domain>
            </identifier-domains>
        </cross-reference-consumer>

        <cross-reference-consumer>
            <name>NIST PIX PDQ Tool HL7v2</name>
            <enabled>true</enabled>
            <!-- device-id must match "xconfig.xml" configuration for endpoints -->
            <!-- device-id = application, device-name = facility -->
            <!-- no endpoints are stored here -->
            <device-id>NIST_RCVR_BTHUMAN</device-id>
            <device-name>NIST</device-name>
            <!--
            FIXME: Add different triggers (add/update/merge @ a later date). Right now, we just
