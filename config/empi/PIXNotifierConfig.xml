            <identifier-domains>
                <identifier-domain>
                    <universal-id>1.3.6.1.4.1.21367.13.20.1000</universal-id>
                    <universal-id-type>ISO</universal-id-type>
                    <namespace-id>IHE_1000</namespace-id>
                </identifier-domain>
                <identifier-domain>
                    <universal-id>2.16.840.1.113883.3.72.5.9.1</universal-id>
